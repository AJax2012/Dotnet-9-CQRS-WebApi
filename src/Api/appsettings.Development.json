{
  "Serilog": {
    "Using": [
      "Serilog.Exceptions",
      "Serilog.Expressions",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Auth": {
    "ClientOrigin": "localhost:7124",
    "JwtBearerTokenSettings": {
      "Audience": "https://localhost:7124",
      "Issuer": "https://localhost:7124",
      "SigningKey": "",
      "ExpiryTimeInSeconds": 60
    }
  }
}
