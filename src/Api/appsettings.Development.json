{
  "Serilog": {
    "Using": [
      "Serilog.Exceptions",
      "Serilog.Expressions",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "http://localhost:5229",
          "https://localhost:7124"
        ],
        "ValidIssuer": "dotnet-user-jwts"
      }
    },
    "ClientOrigin": "http://localhost:*"
  }
}